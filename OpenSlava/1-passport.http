  "includeReceipt": true,
  "authority": "{{authority}}",
  "registration": {
    "clientName": "Airport Passport Verifier"
  },
  "callback": {
    "url": "{{webhook}}",
    "state": "my-verification-state",
    "headers": {
      "api-key": "my-api-key"
    }
